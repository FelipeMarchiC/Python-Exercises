contagem = int(input())
contador = 1
maior = 0
menor = 0
soma = 0

while True:
  x = int(input())
  if contador > 0:
    maior = x
    menor = x
    contador = contador - 1
  if x > maior:
    maior = x
  if x < menor:
    menor = x
  soma += x
  if contagem == 1:
    print(maior)
    print(menor)
    print(soma)
    break
  contagem = contagem - 1
