x = 0
media = 0
maioridade = 0
idosos = 0
individuos = -1

while x >= 0:
  media += x
  if x >= 18:
   maioridade += 1
  if x > 75:
    idosos += 1
  individuos += 1
  x = int(input())

media = media / individuos
idosos = idosos / individuos * 100

print(f"{media:.2f}")
print(maioridade)
print(f"{idosos:.2f}%")
